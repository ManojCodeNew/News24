.smallCardText-container {
  margin-left: 3%;
  height: auto;
  padding: 10px;
}
.smallCard-category{
    background-color: #0066ff;
    padding: 2%;
    text-align: center;
    border-radius: 3px;
    color: white;
}
.smallCard-date{
    font-size: 12px;
}
