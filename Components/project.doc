*{
    margin: 0;
    padding: 0;
    box-sizing: border-box;

}
.NewsDetail-container{
    font-family: Arial, sans-serif;

width: 100%;
height: 90vh;
border: 1px solid black;
}
.NewsDetail-mainContent{
    margin: 2%;
    border: 1px solid black;
    border-radius: 10px;

}
.newsUploaded-Details{
    display: flex;
    justify-content:space-between;
    margin: 1% 1%;
}
.NewsDetail-subContent{
    margin: 2%;
    border: 1px solid black;
    border-radius: 10px;
    height: auto;
}
.NewsImg{
    margin: 2%;
    border: 1px solid black;
    border-radius: 10px;
    height: auto;

}
.NewsImg img{
    width: 50%;
    max-height: 30vh;
    border-radius: 10px;
    object-fit: fill;
    height: auto;
}
.relatedNews{

    margin-left: 1%;
    border: 1px solid black;
}

