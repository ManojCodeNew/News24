//
.NewsCard-container {
  width: 100%;
  max-width: 1200px;
  /* margin: 20px auto; */
  display: grid;
  grid-template-columns:repeat(2, 1fr);
  padding: 20px;
  grid-template-rows: repeat(1,auto);
  grid-gap: 20px;
}

.large-NewsCard {
  grid-column: 1/ span 2;
  grid-row: 1/span 2;
  position: relative;
  border-radius: 10px;
  overflow: hidden;
  height: auto;
}

.NewsImg-large {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

.content-container-large {
  position: absolute;
  bottom: 20px;
  left: 20px;
  color: white;
  background-color: rgba(0, 0, 0, 0.5);
  padding: 10px;
  border-radius: 5px;
}

.category-large {
  display: inline-block;
  padding: 5px 15px;
  background-color: #1e90ff;
  border-radius: 5px;
  font-size: 14px;
}

.headline-large {
  font-size: 28px;
  font-weight: bold;
  margin: 10px 0;
}

.releaseDate-large {
  font-size: 14px;
  color: #dcdcdc;
}

.NavigationLink {
  text-decoration: none;
  display: inline-block;
  width: 100%;
  height: fit-content;
  /* background-color: #1e90ff; */
}

.small-NewsCard {
  display: flex;
  border-radius: 10px;
  background-color: #f9f9f9;
  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
  height: fit-content;
}
/* Layout adjustments */

.NewsImg-small {
  width: 30%;
  /* height: 80%; */
  object-fit: cover;
  border-radius: 5px;
}

.content-container-small {
  padding: 20px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.category-small {
  display: inline-block;
  padding: 5px 10px;
  background-color: #1e90ff;
  border-radius: 5px;
  font-size: 12px;
  color: white;
}

.headline-small {
  font-size: 20px;
  font-weight: bold;
  margin: 10px 0;
  color: #333;
}

.releaseDate-small {
  font-size: 12px;
  color: gray;
}

@media (max-width: 768px) {
  .NewsCard-container {
    grid-template-columns: 1fr;
    grid-gap: 20px;
  }

  .large-NewsCard {
    height: 300px;
  }

  .headline-large {
    font-size: 24px;
  }

  .small-NewsCard {
    height: 180px;
  }
}

@media (max-width: 480px) {
  .NewsCard-container {
    grid-gap: 15px;
  }

  .large-NewsCard {
    height: 250px;
  }

  .headline-large {
    font-size: 22px;
  }

  .small-NewsCard {
    height: 120px;
  }

  .headline-small {
    font-size: 16px;
  }
}


