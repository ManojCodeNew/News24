  //NewsCard Style
  .NewsCard-container {
  width: 70vw;
  height: 50vh;
  overflow: auto;
  padding: 3%;
  color: rgb(0, 0, 0);
  background-color: rgb(216, 215, 215);
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  grid-template-rows: repeat(3, 1fr);
  gap: 20px;
}
