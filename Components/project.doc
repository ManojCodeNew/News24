.card.large {
  position: relative;
  grid-column: span 2;
  grid-row: span 2;
}

.card.large img {
  width: 100%;
  height: 100%;
  border-radius: 5px;
  object-fit: cover;
}


.text-overlay {
  position: absolute;
  bottom: 10px;
  left: 10px;
  color: white;
  text-shadow: 0px 0px 5px gray;
}
.text-overlay .category {
  background-color: #0c0c0cb8;
  padding: 5px 10px;
  border-radius: 5px;
  font-size: 12px;
}
.text-overlay h2,
.text-overlay h3 {
  margin: 5px 0;
}
.text-overlay h2 {
  font-size: 22px;
}
.text-overlay h3 {
  font-size: 18px;
}
.text-overlay .date {
  font-size: 12px;
}

